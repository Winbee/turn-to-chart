{
  "name": "turn-to-chart",
  "version": "1.0.0",
  "homepage": "https://github.com/Winbee/turn-to-chart",
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/turn-to-chart.cjs",
  "module": "./dist/turn-to-chart.js",
  "exports": {
    ".": {
      "import": "./dist/turn-to-chart.js",
      "require": "./dist/turn-to-chart.umd.cjs"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "build:docs": "vite build --config vite-docs.config.ts",
    "preview:docs": "vite preview  --config vite-docs.config.ts",
    "test": "TZ=UTC vitest"
  },
  "keywords": [
    "csv",
    "markdown",
    "graph",
    "chart",
    "ui"
  ],
  "dependencies": {
    "@visx/axis": "^2.14.0",
    "@visx/clip-path": "^2.10.0",
    "@visx/group": "^2.10.0",
    "@visx/legend": "^2.10.0",
    "@visx/scale": "^2.2.2",
    "@visx/shape": "^2.12.2",
    "d3-format": "^3.1.0",
    "d3-scale-chromatic": "^3.0.0",
    "d3-time-format": "^4.1.0",
    "papaparse": "^5.3.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/d3-format": "^3.0.1",
    "@types/d3-scale-chromatic": "^3.0.0",
    "@types/d3-time-format": "^4.0.0",
    "@types/papaparse": "^5.3.5",
    "@types/prettier": "^2.7.1",
    "@types/react": "^18.0.21",
    "@types/react-dom": "^18.0.6",
    "@vitejs/plugin-react": "2.1.0",
    "prettier": "^2.7.1",
    "typescript": "^4.6.4",
    "vite": "^3.1.0",
    "vitest": "^0.24.3"
  }
}
